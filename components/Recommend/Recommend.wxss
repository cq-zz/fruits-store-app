.recommend-block-box {
  width: 100%;
  padding: 10rpx 0;
}
.recommend-block-box .title-img {
  height: 40rpx;
  margin-left: 20rpx;
}
.recommend-block-box .content-box {
  height: 320rpx;
  overflow: hidden;
}
.recommend-block-box .content-box navigator {
  float: left;
  width: 33.33%;
  height: 50%;
  padding: 10rpx;
}
.recommend-block-box .content-box navigator:nth-of-type(1) {
  height: 100%;
}
.recommend-block-box .content-box navigator image {
  width: 100%;
  height: 100%;
}
